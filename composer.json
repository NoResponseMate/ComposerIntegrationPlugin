{
	"name": "no-response-mate/composer-integration-plugin",
	"description": "Quick switching between integrations' dependencies",
	"type": "composer-plugin",
	"license": "MIT",
	"authors": [
		{
			"name": "Jan GÃ³ralski",
			"email": "jan.wojciech.goralski@gmail.com"
		}
	],
	"require": {
		"php": "^8.0",
		"composer-plugin-api": "^2.0"
	},
	"require-dev": {
		"composer/composer": "^2.0",
		"phpunit/phpunit": "^9.5",
		"vimeo/psalm": "^4.19"
	},
	"autoload": {
		"psr-4": {
			"ComposerIntegration\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\ComposerIntegration\\": "tests/"
		}
	},
	"extra": {
		"class": "ComposerIntegration\\Plugin",
		"branch-alias": {
			"dev-main": "1.x-dev"
		}
	},
	"config": {
		"sort-packages": true
	}
}
