{
    "name": "no-response-mate/with-local-env",
    "description": "Test project",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "composer/semver": "~3.2.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../../*"
        }
    ],
    "require-dev": {
        "no-response-mate/composer-integration-plugin": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "ComposerIntegration\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "no-response-mate/composer-integration-plugin": true
        }
    },
    "extra": {
        "integration": {
            "with-different-env": {
                "require": {
                    "league/uri": "^6"
                },
                "env": "test_with_env"
            },
            "with-same-env": {
                "require": {
                    "league/uri": "^6"
                },
                "env": "test"
            }
        }
    }
}
